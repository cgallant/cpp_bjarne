#include "std_lib_facilities.h"

using namespace std;

int main() {
	double a, b;

	while (cin >> a >> b) {
		cout << a << "  " << b << endl;
		if (b < a) {
			double temp;
			temp = a;
			a = b;
			b = temp;
		}
		cout << "The smaller value is: " << a << endl;
		cout << "The larger value is: " << b << endl;
	}
	cout << "end" << endl;
	return 0;
}
